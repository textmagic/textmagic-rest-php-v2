{
    "name": "textmagic/textmagic-rest-php-v2",
    "version": "3.0.43870",
    "description": "PHP SDK for TextMagic API",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api",
        "textmagic",
        "sms"
    ],
    "homepage": "https://www.textmagic.com",
    "license": "MIT",
    "authors": [
        {
            "name": "TextMagic",
            "email": "developers@textmagic.biz",
            "homepage": "https://www.textmagic.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.8",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "TextMagic\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "TextMagic\\Test\\" : "test/" }
    }
}
